{
  "name": "venice-caching-tests",
  "version": "2.0.0",
  "description": "Venice.ai prompt caching test suite and health monitor",
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:unit": "bun test tests/unit",
    "test:integration": "bun test tests/integration",
    "test:utils": "bun test tests/unit/utils",
    "test:core": "bun test tests/unit/core",
    "test:db": "bun test tests/unit/db",
    "venice": "bun run index.ts",
    "venice:watch": "bun --watch run index.ts",
    "venice:quick": "bun run quick-test.ts",
    "db:init": "bun run src/db/cli.ts run",
    "db:migrate": "bun run src/db/cli.ts run",
    "db:migrate:status": "bun run src/db/cli.ts status",
    "server": "bun run server.ts",
    "server:dev": "bun --watch run server.ts"
  },
  "dependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "latest"
  }
}
